{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es2022", // Update the value to es2022
    "module": "commonjs",
    "outDir": "dist",
    "strict": true,
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop":true,
    "allowJs": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "./node_modules/**/*"
  ]
}